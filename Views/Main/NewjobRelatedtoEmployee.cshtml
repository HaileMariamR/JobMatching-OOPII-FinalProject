@model List<Models.projectModels.Job>;



@{
    
    Layout ="~/Views/Shared/_MainViewLayout.cshtml";

}


<div class="row">
    <div class="col-md-12">
        <h1>all application related to ur profile</h1>
    </div>

</div>

<div>

        @if (@Model != null){

            


                @foreach(var item in Model)

                    {
                        
                            <br><br>
                    <br><br>
                    <br><br>
                    <br><br>


                        <div class="card" style="">
                    <div class="card-body">
                        <h5 class="card-title">@item.JobTitle</h5>
                        <h1 >@item.CompanyName</h1  >
                    </div>
                    </div>
                    <br><br>
                    <br><br>
                    <br><br>
                        
            }
        }
        else{

            <h1>U havent any application here!</h1>
        }


</div>